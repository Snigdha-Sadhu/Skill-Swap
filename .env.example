# MongoDB connection string - replace with your own connection string
MONGO_URL=mongodb+srv://username:password@cluster0.mongodb.net/yourdbname

# Backend server port - real port number (not secret)
PORT=7000

# JWT secret key - replace with a strong secret
JWT_SECRET=your_jwt_secret_here

# Frontend client URL (used for CORS)
CLIENT_URL=http://localhost:5173
